<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_dashboarddokter"
        android:name="com.example.kneecheck.dokter.DashboardDokterFragment"
        tools:layout="@layout/fragment_dashboard_dokter" />

    <fragment
        android:id="@+id/navigation_scandokter"
        android:name="com.example.kneecheck.dokter.ScanDokterFragment"
        tools:layout="@layout/fragment_scan_dokter" />

    <fragment
        android:id="@+id/navigation_historydokter"
        android:name="com.example.kneecheck.dokter.HistoryDokterFragment"
        tools:layout="@layout/fragment_history_dokter" />

    <fragment
        android:id="@+id/navigation_profiledokter"
        android:name="com.example.kneecheck.dokter.ProfileDokterFragment"
        tools:layout="@layout/fragment_profile_dokter" />
</navigation>
